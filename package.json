{
  "name": "typescript-express-mongo-starter",
  "version": "1.0.0",
  "description": "Express MongoDb Starter-kit written in typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/morlombakop/typescript-express-mongo-starter.git"
  },
  "author": "Morlo Mbakop <morlombakop@gmail.com>",
  "license": "MIT",
  "scripts": {
    "develop": "npm run build && npm run start",
    "test": "jest --no-cache",
    "start": "node dist/src/app.js",
    "copy:static-assets": "ts-node copyStaticAssets.ts",
    "build:ts": "tsc",
    "build": "npm run clean:dist && npm run build:ts && npm run copy:static-assets",
    "clean:dist": "rimraf dist"
  },
  "main": "src/app.ts",
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "MongoDb",
    "boilerplate",
    "skeleton",
    "starter-kit"
  ],
  "homepage": "https://github.com/morlombakop/typescript-express-mongo-starter#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-status-monitor": "^1.2.6",
    "helmet": "^3.18.0",
    "microframework-w3tec": "^0.6.3",
    "mongoose": "^5.6.0",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/ejs": "^2.6.3",
    "@types/express": "^4.17.0",
    "@types/mongoose": "^5.5.6",
    "@types/shelljs": "^0.8.5",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "typescript": "^3.5.2"
  }
}
